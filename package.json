{
  "name": "@qelements/ui",
  "version": "1.0.0",
  "description": "A powerful TypeScript/TSX package for parent-child UI element management with cascading styles, themes, responsive design, animations, and performance monitoring",
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc --watch",
    "clean": "find dist -type f -delete && rmdir dist 2>/dev/null || true",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No linter configured\" && exit 0"
  },
  "keywords": [
    "ui",
    "typescript",
    "react",
    "tsx",
    "components",
    "styling",
    "themes",
    "responsive",
    "animations",
    "performance",
    "parent-child",
    "cascading",
    "inheritance",
    "overrides"
  ],
  "author": "QElements Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cysectools/QElement.git"
  },
  "homepage": "https://github.com/cysectools/QElement#readme",
  "bugs": {
    "url": "https://github.com/cysectools/QElement/issues"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "files": [
    "build",
    "README.md",
    "examples"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
